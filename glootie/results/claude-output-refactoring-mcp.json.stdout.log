\n    setLoading(true)\n    setError(null)\n\n    try {\n      const result = await asyncFn()\n      setData(result)\n      onSuccess?.(result)\n      return result\n    } catch (error) {\n      const errorMessage = error instanceof Error ? error.message : 'An unknown error occurred'\n      setError(errorMessage)\n      onError?.(error instanceof Error ? error : new Error(errorMessage))\n      throw error\n    } finally {\n      setLoading(false)\n      onSettled?.()\n    }\n  }\n}"}}],"stop_reason":null,"stop_sequence":null,"usage":{"input_tokens":492,"output_tokens":918,"cache_read_input_tokens":37177}},"parent_tool_use_id":null,"session_id":"ea3b70fd-130c-440d-8602-90050a39ea94","uuid":"9b547947-cbfd-40b2-91cd-32ae76ac4563"}
