{"type":"assistant","message":{"id":"msg_20250926195726b97c64f813274f26","type":"message","role":"assistant","model":"glm-4.5","content":[{"type":"tool_use","id":"call_44k1vyt7b8d","name":"Write","input":{"file_path":"/config/workspace/optimized-test-1758887759363-mcp-refactoring/src/stores/utils/store-utils.ts","content":"import { z } from 'zod'\n\nexport function createSafeAction<T extends z.ZodType<any, any>>(\n  schema: T,\n  handler: (data: z.infer<T>) => Promise<any> | any\n) {\n  return async (input: unknown) => {\n    try {\n      const validatedData = schema.parse(input)\n      return await handler(validatedData)\n    } catch (error) {\n      if (error instanceof z.ZodError) {\n        throw new Error(`Validation failed: ${error.errors.map(e => e.message).join(', ')}`)\n      }\n      throw error\n    }\n  }\n}\n\nexport function createRetryableAction<T>(\n  action: () => Promise<T>,\n  maxRetries = 3,\n  delay = 1000\n): Promise<T> {\n  return new Promise((resolve, reject) => {\n    let attempts = 0\n\n    const attempt = () => {\n      attempts++\n      action()\n        .then(resolve)\n        .catch((error) => {\n          if (attempts < maxRetries) {\n            setTimeout(attempt, delay * attempts)\n          } else {\n            reject(error)\n          }\n        })\n    }\n\n    attempt()\n  })\n}\n\nexport function debounceAction<T extends any[]>(\n  action: (...args: T) => void,\n  delay = 300\n): (...args: T) => void {\n  let timeoutId: NodeJS.Timeout\n  return (...args: T) => {\n    clearTimeout(timeoutId)\n    timeoutId = setTimeout(() => action(...args), delay)\n  }\n}\n\nexport function throttleAction<T extends any[]>(\n  action: (...args: T) => void,\n  limit = 300\n): (...args: T) => void {\n  let inThrottle: boolean\n  return (...args: T) => {\n    if (!inThrottle) {\n      action(...args)\n      inThrottle = true\n      setTimeout(() => (inThrottle = false), limit)\n    }\n  }\n}\n\nexport function withLocalStorage<T>(\n  key: string,\